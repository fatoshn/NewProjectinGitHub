.altmetin a:hover { font: 10px Georgia, Helvetica, sans-serif; color:#FF0000;}
#sayac{ position:relative; float:left; width: 140px; height: 80px; margin-left:auto; margin-right:auto; padding-top:40px; padding-left:20px; overflow:hidden; }

.yankutubaslik { width: 170px; height:15px; font: 12px Georgia, Arial, Helvetica, sans-serif; color:#FFFFFF; font-weight: bold; padding: 5px; background-color:#7C1900; border-bottom: solid 1px #7C1900; }
.yankutumetin { width: 200px; font: 14px Georgia, Helvetica, sans-serif; margin-bottom:10px; text-align:left; background-color:#FFFFFF; overflow:hidden;  }
#osmanlibaslik { width: 180px; height:25px; background-image:url(img/osmanli_baslik.gif); }
#tarihbaslik { width: 180px; height:25px; background-image:url(img/tarih_baslik.gif); }
#mehterhanebaslik { width: 180px; height:25px; background-image:url(img/mehterhane_baslik.gif); }
#kutuphanebaslik { width: 180px; height:25px; background-image:url(img/kutuphane_baslik.gif); }

.listmenu { position: relative; margin-bottom: 5px;  padding: 10px 10px 10px 10px;  color:#333; font-size: 12px;}
.listmenu ul { margin: 0px; padding: 0px;}
.listmenu ul li { margin: 0px; padding: 2px; list-style-type: none; font-size: 11px; }
.listmenu ul li a { background: url(img/ok.png) no-repeat left top; padding: 3px 0px 3px 10px; font-size: 12px; margin:2px; }
.listmenu ul li a:visited { background: url(img/ok.png) no-repeat left top; }
.listmenu ul li a:hover { background: #ffffff url(img/ok.png) no-repeat left top; text-decoration: none;}

#padisahmenu {width: 250px; overflow:hidden; margin-left:auto; margin-right:auto; border-bottom:solid 1px #999999; margin-bottom:10px;}
#padisahmenu ul { margin: 0px; padding: 2px;}
#padisahmenu ul li { margin: 0px; padding: 2px; list-style-type: none; font-size: 11px; }
#padisahmenu ul li a { background: url(img/ok.png) no-repeat left top; padding: 2px 0px 2px 10px; font-size: 12px; }
#padisahmenu ul li a:visited { background: url(img/ok.png) no-repeat left top; }
#padisahmenu ul li a:hover { background: #ffffff url(img/ok.png) no-repeat left top; text-decoration: none;}

.icerikbaslik { font-family: Georgia, Helvetica, sans-serif; font-size:12px; letter-spacing:1pt; color:#7C1900; text-align:center; padding-bottom:10px;  }
.icerikbaslik a {color:#7C1900;}
.icerikbaslik a:hover {color:#FF0000;}
.icerikmetin { margin:auto; padding:10px; word-spacing:2pt; line-height:1.8;}

.icerikaltbaslik {font: 14px Georgia, Helvetica, sans-serif; font-weight:bold; letter-spacing:1pt; color:#7C1900; padding:10px; }

.padisahbaslik {font: 22px Georgia, Helvetica, sans-serif; text-align:center; color:#7C1900; border-bottom:1px solid #cccccc; padding:10px; }
.buyukbaslik {font: 36px Georgia, Helvetica, sans-serif; text-align:center; color:#7C1900; padding-top:20px; letter-spacing:2pt; line-height:1.8; }
.buyukbaslikturkistan {font: 24px Georgia, Helvetica, sans-serif; text-align:center; color:#7C1900; padding-top:20px; letter-spacing:2pt; line-height:1.8; }

.padisahbaslik a {color:#7C1900;}
.padisahbaslik a:hover {color:#FF0000;}
.padisahicerikmetin { margin:auto; padding-left:30px; padding-right:30px; padding-bottom:30px; word-spacing:2pt; line-height:1.8; text-align:justify;}
.tugra { margin-left:auto; margin-right:auto; text-align:center; }

#iceriksagmenu { float:left; width: 180px; overflow:hidden; padding-top:20px; padding-bottom:20px; padding-right:20px; margin-left:auto; margin-right:auto; background-color:#FFFFFF; }
#iceriksagmenu a {font: 12px Georgia, Helvetica, sans-serif; color:#333333; display:block; border-bottom:solid 1px #FFCC99; margin:1px; padding:1px; line-height:1.5;}
#iceriksagmenu a:hover { color:#CC0000; background-color:#ffffff; border-bottom:solid 1px #CC0000;}


