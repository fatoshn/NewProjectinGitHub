/* OSMANLILAR */

body {margin:auto; padding:auto; color:#000000; background:url(images/bg.jpg) 50% 0% no-repeat; font-family: Trebuchet MS, Arial, Helvetica, sans-serif;font-size:12px;}
a { text-decoration:none; list-style-type:none; color:#D94E40;}
a:hover { text-decoration:none; list-style-type:none; color:#000000;}

/* ÜST */
#osmanlilar { position:relative; top: 7px; width: 998px; height: 77px; margin-left:auto; margin-right:auto; }
#menu { position:relative; top: 255px; width: 100%; height: 40px; text-decoration:none; text-align:center; padding:5px; letter-spacing:1pt; }
#feed {float:right;}

#sayfa { width: 813px; margin-top:25px; height: auto; overflow:hidden; margin-left:auto; margin-right:auto; background-color:transparent; left: -12px;position: relative;bottom:24px;}
#icerik { float:left; width: 960px; overflow:hidden; padding:20px; margin-left:auto; margin-right:auto; background-color:#FFFFFF; text-align:justify; }
/* SOL */

#sol {  float:left;width: 190px; height: 450px; overflow:hidden;  padding: 20px 6px 20px 8px;}
#ust {  float:left;width: 813px; height: 50px; margin-top:148px; overflow:hidden;  padding: 20px 6px 10px 0px;}

.menu { margin:auto; padding:auto; }
.menu a {font: 14px Georgia, Helvetica, sans-serif; color:#7C1900; display:block; border-bottom:solid 1px #D0FDFE; margin:1px; padding:1px; }
.menu a:hover { color:#CC0000; background-color:#ffffff; border-bottom:solid 1px #7C1900;}

/* sol duyuru */
#solduyuru {  float:left;width: 200px; overflow:hidden; background-color:#FFFFFF;  padding: 2px 2px 2px 2px;}

/* SAG */
#sag { position:relative; top: 2px; float:left; width: 310px; overflow:hidden; background-color:#FFFFFF; padding: 17px 6px 40px 8px; }

/* ORTA1 */
#orta1 {  width: 998px; height: 246px; overflow:hidden;  background-color:#FFFFFF; padding: 0px; }
.ortakutu1 {width: 998px; overflow:hidden; border-bottom:solid 0px #999999; margin-bottom:2px; }

/* ORTA2 */
#orta2 {  width: 998px; overflow:hidden;  background-color:#FFFFFF; padding: 0px; }
.ortakutu {width: 998px; overflow:hidden; border-bottom:solid 1px #999999; margin-bottom:2px; }

/* ORTAmeda */
#ortameda {  width: 990px; overflow:hidden;  background-color:#FFFFFF; padding: 0px; }

/* ORTA RESİM */

#ortaresim { position:relative; top: 5px; float:left; width: 790px; overflow:hidden; background-color:#FFFFFF; padding: 12px; }


/* ORTA */
#orta { position:relative; top: 5px; float:left; width: 760px; height: auto; overflow:hidden;  padding: 10px; }
.ortakutu {width: 310px; overflow:hidden; border-bottom:solid 1px #7C1900; margin-bottom:10px; }
.ortakutuic {width: 950px; overflow:hidden; border-bottom:solid 0px #7C1900; margin-bottom:10px; padding: 30px 30px 40px 30px; }
.ortakutua {width: 300px; overflow:hidden; border-bottom:solid 1px #7C1900; margin-bottom:10px; }
.ustbaslik {font: 12px Arial, Helvetica, sans-serif; text-align:right; font-weight:bold; color:#7C1900; }
.baslik {font: 22px Georgia, Helvetica, sans-serif; color:#7C1900; letter-spacing:1pt;}
.baslik a {color:#7C1900;}
.basharf {color:#7C1900;display:inline;float:left;font-size:2.8em;line-height:0.8em;text-transform:uppercase;margin:0.07em 0.14em 0pt 0pt;}
.basharf a {color:#7C1900;}
.basharf a:hover {color:#FF0000;}
.baslik a:hover {color:#FF0000;}
.altbaslik {font: 12px Arial, Helvetica, sans-serif; font-weight:bold; color:#7C1900; padding-top:2px; padding-bottom:2px;}
.metin {font: 14px Georgia, Helvetica, sans-serif; margin-bottom:2px; word-spacing:1pt; line-height:1.8;  }
.metin a {color:#7C1900;}
.metin a:hover {color:#FF0000;}


/* kamer */
#kamer { width: 100%; top: 0px; height: 70px;  }
#ortakamer{ width: 998px; height: 70px; margin-left:auto; margin-right:auto; background-image:url(img/menu.gif); background-repeat:repeat-x; }

/* meda */
#meda { width: 100%; top: 0px; height: 70px; background-image:url(img/2kamer.gif); background-repeat:repeat-x; }


/* ALT */
#alt { width: 100%; height: 170px; background-image:url(img/altbak.gif); background-repeat:repeat-x; }
#altosmanlilar{ position:relative; top: 85px; width: 998px; height:75px; margin-left:auto; margin-right:auto;  background-repeat:repeat-x; }
#altmetin{ position:relative; float:left; width: 700px; text-align:center; margin-left:auto; margin-right:auto; padding-left:240px; padding-top:120px; padding-right:40px; overflow:hidden; }
.altmetinic{ font: 10px Georgia, Helvetica, sans-serif; color:#7C1900; }
